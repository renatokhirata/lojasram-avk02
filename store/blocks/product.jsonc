    "props": {
      "blockClass": "shipping",
      "text": "**FRETE GRÁTIS** ACIMA DE R$ 899"
    }
  },
  "flex-layout.row#price": {
    "props": {
      "colSizing": "equal",
      "blockClass": "price"
    },
    "children": [
      "flex-layout.col#selling-price",
      "billetPrice"
    ]
  },
  "flex-layout.col#selling-price": {
    "props": {
      "blockClass": "selling-price",
