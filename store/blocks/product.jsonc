{
  "store.product": {
    "children": [
      "responsive-layout.desktop#product",
      "responsive-layout.mobile#product"
    ]
  },
  "responsive-layout.desktop#product": {
    "children": [
      "flex-layout.row#product-main",
      "flex-layout.row#product-context-menu",
      "flex-layout.row#product-context",
      "flex-layout.row#product-specifications",
