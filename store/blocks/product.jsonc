    ]
  },
  "product-installments": {
    "props": {
      "interestRate": "",
      "hasInterest": false
    }
  },
  "flex-layout.row#selling-price-row": {
    "props": {
      "colSizing": "auto",
      "blockClass": "selling-price-row"
    },
    "children": [
      "product-list-price",
      "product-selling-price"
      // "product-price-savings"
    ]
  },
  "flex-layout.row#buy-button": {
    "props": {
      "colSizing": "auto",
      "blockClass": "buy-button"
    },
    "children": [
      "add-to-cart-button#pdp"
      // "modal-trigger#list-installments"
    ]
  },
  "add-to-cart-button#pdp": {
    "props": {
      "text": "Comprar",
      "isOneClickBuy": "true"
    }
  },
  "modal-trigger#list-installments": {
    "children": [
      "rich-text#list-installments",
